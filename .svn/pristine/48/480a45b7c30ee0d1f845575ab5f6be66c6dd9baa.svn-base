@font-face {
  font-family: 'SUIT';
  font-weight: 900;
  src:
    local('SUIT Heavy'),
    url('../assets/fonts/SUIT-Heavy.woff2') format('woff2');
}

@mixin blind {
  position: absolute;
  width: 1px;
  height: 1px;
  clip: rect(0 0 0 0);
  overflow: hidden;
}

body {
  font-family: 'SUIT', sans-serif;
  word-break: keep-all;
}

svg {
  fill: inherit;
}

.text-divider {
  &:before {
    display: inline-block;
    content: '|';
    margin: 0 8px;
  }
}

