egmentValue !== \"light\") {\r\n        this.getSendLetterList();\r\n      } else {\r\n        this.getPostLetterList();\r\n      }\r\n    }\r\n  }\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.example-content {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 100%;\r\n}\r\n\r\n.alarm {\r\n  position: absolute;\r\n  top: 1px;\r\n  right: -5px;\r\n  font-size: 4px;\r\n}\r\n\r\nion-toolbar {\r\n  --background: #fff;\r\n}\r\n\r\nion-icon {\r\n  color: black;\r\n}\r\n\r\n.letter-sticker::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  top: 8px;\r\n  left: 20px;\r\n  width: 15px;\r\n  height: 15px;\r\n  border-radius: 50%;\r\n  background-color: red;\r\n}\r\n</style>\r\n  ","import { render } from \"./Letter.vue?vue&type=template&id=c48f1e6c&scoped=true\"\nimport script from \"./Letter.vue?vue&type=script&lang=js\"\nexport * from \"./Letter.vue?vue&type=script&lang=js\"\n\nimport \"./Letter.vue?vue&type=style&index=0&id=c48f1e6c&scoped=true&lang=css\"\n\nimport exportComponent from \"../../../node_modules/vue-loader/dist/exportHelper.js\"\nconst __exports__ = /*#__PURE__*/exportComponent(script, [['render',render],['__scopeId',\"data-v-c48f1e6c\"]])\n\nexport default __exports__"],"names":["_createElementVNode","class","alt","src","style","_createBlock","_component_ion_page","id","_createVNode","_component_ion_header","_component_ion_toolbar","_component_ion_buttons","slot","_component_ion_button","onClick","_cache","$event","_ctx","$router","go","_component_ion_icon","icon","$data","chevronBack","push","size","storefrontOutline","notificationsOutline","_hoisted_1","_component_ion_card","_component_ion_card_content","_hoisted_2","_component_ion_title","_hoisted_3","_hoisted_4","_component_ion_content","_component_ion_segment","segmentValue","_component_ion_segment_button","value","$options","changeSegment","_component_ion_label","key","_component_ion_list","lines","_createElementBlock","_Fragment","_renderList","postLi