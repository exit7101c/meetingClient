lert = await this.alertController.create({\r\n        header: \"\",\r\n        subHeader: \"\",\r\n        message: message,\r\n        buttons: [\"OK\"]\r\n      });\r\n      return alert.present();\r\n    },\r\n    //알람정보 수정\r\n    setSettingUserSet(e, codeType) {\r\n      getData({\r\n        url: \"/setSettingUserTerm\",\r\n        param: { codeType: codeType },\r\n        then: (data) => {\r\n          if (data.successYn === \"N\") {\r\n            this.warningAlert(data.message);\r\n          } else {\r\n            if (codeType === \"MARKETING\") {\r\n              this.alarmList[0].valBoolean =\r\n                data.updatedValue === \"Y\" ? true : false;\r\n            } else if (codeType === \"PUSH\") {\r\n              this.alarmList[1].valBoolean =\r\n                data.updatedValue === \"Y\" ? true : false;\r\n            }\r\n          }\r\n        }\r\n      });\r\n    }\r\n  }\r\n});\r\n</script>\r\n<style scoped></style>\r\n","import { render } from \"./SettingService.vue?vue&type=template&id=235da688\"\nimport script from \"./SettingService.vue?vue&type=script&lang=js\"\nexport * from \"./SettingService.vue?vue&type=script&lang=js\"\n\nimport exportComponent from \"../../../node_modules/vue-loader/dist/exportHelper.js\"\nconst __exports__ = /*#__PURE__*/exportComponent(script, [['render',render]])\n\nexport default __exports__"],"names":["_createBlock","_component_ion_item","button","class","_normalizeClass","$props","icon","mode","_component_ion_icon","slot","color","_createVNode","_component_ion_text","title","name","props","type","String","__exports__","render","_createElementVNode","item","setNm","_hoisted_1","setDesc","_component_ion_toggle","checked","valBoolean","onClick","$options","setSettingUserSet","Object","methods","this","$emit","_component_ion_page","_component_ion_header","_component_ion_toolbar","_component_ion_buttons","_component_BackButton","_component_ion_content","_component_CardBox","padding","shadow","_component_ion_list"